{
  "name": "basic",
  "version": "0.1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "test": "npm run build:testConfig && cross-env CYPRESS_baseUrl=http://localhost:3000 cypress run",
    "test:ui": "npm run build:testConfig && cross-env CYPRESS_baseUrl=http://localhost:3000 cypress open"
  },
  "dependencies": {
    "firebase": "^6.6.1",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "cypress": "^3.4.1",
    "cypress-firebase": "0.2.5",
    "eslint-plugin-cypress": "^2.6.1",
    "react-scripts": "3.1.1",
    "firebase-tools": "^7.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "private": true
}
